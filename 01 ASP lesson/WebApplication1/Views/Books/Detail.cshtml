
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Detail</title>
</head>
<body>
    <div>
            <div class="card my-card">
                <img src="@ViewBag.Detail.Img" alt="Alternate Text" />
                <div class="card-title">@ViewBag.Detail.Title</div>
                <div class="card-text">@ViewBag.Detail.Desc</div>
            </div>
    </div>
</body>
</html>
